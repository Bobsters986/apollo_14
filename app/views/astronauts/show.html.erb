<h2>Astronaut: <%= @astronaut.name %></h2>
<p id="missions">
<strong>Missions: </strong><% @missions.each do |mission| %>
  '<%= mission %>'
<% end %>
</p>

<section id="new_mission">
  <%= form_with url: "/astronauts/#{@astronaut.id}/missions", method: :post, local: true do |form| %>
    <%= form.label :mission_id, "Add Mission" %>
    <%= form.number_field :mission_id %>
    <%= form.submit :Submit %>
  <% end %>
</section>